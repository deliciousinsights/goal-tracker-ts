{
  "name": "goal-tracker-ts",
  "version": "8.0.0",
  "private": true,
  "description": "Demo app (TS version) used in Delicious Insightsâ€™ React PWA training course",
  "author": "Christophe Porteneuve <christophe@delicious-insights.com>",
  "license": "No-License (see LICENSE file)",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:poll": "cross-env CHOKIDAR_USEPOLLING=true npm start",
    "test": "react-scripts test"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 8"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "semi": false,
    "singleQuote": true
  },
  "deliciousImportSort": {
    "customGroups": "@mui",
    "mainOrder": [
      "absolute",
      "relative",
      "bare"
    ]
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "delicious"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "no-restricted-imports": "off",
      "@typescript-eslint/consistent-type-imports": "warn",
      "@typescript-eslint/no-restricted-imports": [
        "warn",
        {
          "name": "react-redux",
          "importNames": [
            "useSelector",
            "useDispatch"
          ],
          "message": "Use typed hooks `useAppDispatch` and `useAppSelector` instead."
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "store.ts"
        ],
        "rules": {
          "@typescript-eslint/no-restricted-imports": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@redux-offline/redux-offline": "^2.6.0",
    "@reduxjs/toolkit": "^1.8.5",
    "bson-objectid": "^2.0.2",
    "date-fns": "^2.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.1.2",
    "web-vitals": "^3.0.2",
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-google-analytics": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.3",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-env": "^1.18.0",
    "body-parser": "^1.19.2",
    "chalk-template": "^0.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.3",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-delicious": "^1.0.2",
    "morgan": "^1.10.0",
    "typescript": "^4.8.4"
  }
}
