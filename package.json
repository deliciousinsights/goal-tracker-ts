{
  "name": "goal-tracker-ts",
  "version": "8.2.0",
  "private": true,
  "description": "Demo app (TS version) used in Delicious Insightsâ€™ React PWA training course",
  "author": "Christophe Porteneuve <christophe@delicious-insights.com>",
  "license": "No-License (see LICENSE file)",
  "scripts": {
    "build": "react-scripts build",
    "deploy": "netlify deploy --prod",
    "doc": "rimraf doc && npx groc",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:all": "npm-run-all --parallel --print-label start start:api",
    "start:api": "node tools/dev-server.mjs",
    "start:poll": "cross-env CHOKIDAR_USEPOLLING=true npm start",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage --watchAll"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 8"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "semi": false,
    "singleQuote": true
  },
  "deliciousImportSort": {
    "customGroups": "@mui",
    "mainOrder": [
      "absolute",
      "relative",
      "bare"
    ]
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "delicious"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "no-restricted-imports": "off",
      "@typescript-eslint/consistent-type-imports": "warn",
      "@typescript-eslint/no-restricted-imports": [
        "warn",
        {
          "name": "react-redux",
          "importNames": [
            "useSelector",
            "useDispatch"
          ],
          "message": "Use typed hooks `useAppDispatch` and `useAppSelector` instead."
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "store.ts"
        ],
        "rules": {
          "@typescript-eslint/no-restricted-imports": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@redux-offline/redux-offline": "^2.6.0",
    "@reduxjs/toolkit": "^1.9.7",
    "bson-objectid": "^2.0.4",
    "date-fns": "^2.30.0",
    "groc": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "react-scripts": "^5.0.1",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.2.1",
    "web-vitals": "^3.5.0",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.6",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/webpack-env": "^1.18.4",
    "body-parser": "^1.20.2",
    "chalk-template": "^1.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.18.2",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-delicious": "^1.0.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  }
}
